{
    "compilerOptions": {
      "target": "ES2022",
      "lib": ["ES2023", "DOM"],  
      "module": "ESNext",
      "skipLibCheck": true,
      "moduleResolution": "node",  
      "isolatedModules": true,
      "moduleDetection": "force",
      "noEmit": true,
      "strict": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "noFallthroughCasesInSwitch": true
    },
    "include": ["vite.config.ts"]
  }
  
